{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Основные разрешения приложения Srecha Invoice",
  "windows": ["*"],
  "permissions": [
    "core:default",
    {
      "identifier": "shell:allow-open",
      "allow": [
        { "url": "https://*" },
        { "url": "http://*" },
        { "url": "tel:*" },
        { "url": "mailto:*" }
      ]
    }
  ]
}
